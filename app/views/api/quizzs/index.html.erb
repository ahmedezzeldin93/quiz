<p id="notice"><%= notice %></p>

<h1>Listing Quizzs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Subject</th>
      <th>Description</th>
      <th>Mark</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quizzs.each do |quizz| %>
      <tr>
        <td><%= quizz.title %></td>
        <td><%= quizz.subject %></td>
        <td><%= quizz.description %></td>
        <td><%= quizz.mark %></td>
        <td><%= quizz.status %></td>
        <td><%= link_to 'Show', quizz %></td>
        <td><%= link_to 'Edit', edit_quizz_path(quizz) %></td>
        <td><%= link_to 'Destroy', quizz, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quizz', new_quizz_path %>
